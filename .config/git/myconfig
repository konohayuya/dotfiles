[alias]
	graph = "log --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --date-order"
[difftool]
	prompt = false
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
	conflictStyle = diff3
[mergetool]
	prompt = false
[fetch]
	prune = true
[pull]
	rebase = merges
